local function getAnyEgg()
	local inventory = player:FindFirstChild("Inventory") or player:FindFirstChild("Backpack")
	if not inventory then return nil end

	for _, item in pairs(inventory:GetChildren()) do
		local itemName = item.Name:lower()
		if itemName:find("egg") then
			return item
		end
	end
	return nil
end

local function autoEatEgg()
	while task.wait(eggDelay) do
		if not autoEgg then break end

		local egg = getAnyEgg()
		if egg then

			if ReplicatedStorage:FindFirstChild("rEvents") and ReplicatedStorage.rEvents:FindFirstChild("proteinEggEvent") then
				ReplicatedStorage.rEvents.proteinEggEvent:FireServer("useEgg", egg.Name)
			else
			  
				if egg.Name == snackName then
					egg.Parent = player.Character
					task.wait(0.1)

					if egg:IsA("Tool") then
						egg:Activate()
					end

					local eatFunction = egg:FindFirstChild("Eat") or egg:FindFirstChild("eat")
					if eatFunction and eatFunction:IsA("RemoteEvent") then
						eatFunction:FireServer()
					elseif eatFunction and eatFunction:IsA("BindableFunction") then
						eatFunction:Invoke()
					elseif eatFunction and eatFunction:IsA("BindableEvent") then
						eatFunction:Fire()
					end
				end
			end
		else
			autoEgg = false
			StarterGui:SetCore("SendNotification", {
				Title = "Auto Egg",
				Text = "Nenhum ovo encontrado no invent√°rio!",
				Duration = 3
			})
			break
		end
	end
end

