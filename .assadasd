end, { placeholder = tostring(totalReps) })

FarmingTab:AddTextBox("T√≥picos", function(value)
    local num = tonumber(value)
    if num and num > 0 then
        threadCount = math.floor(num)
        updateRepsPerTick()
    end
end, { placeholder = tostring(threadCount) })

FarmingTab:AddTextBox("Segundos", function(value)
    local num = tonumber(value)
    if num and num > 0 then
        durationSeconds = num
        updateRepsPerTick()
    end
end, { placeholder = tostring(durationSeconds) })

