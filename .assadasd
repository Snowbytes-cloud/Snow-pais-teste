	local spanishWorkoutName = workoutTranslations[workoutType]

	local dropdown = farmGymsFolder:AddDropdown(spanishWorkoutName .. " - Gym", function(selected)
		_G["selected" .. string.gsub(workoutType, " ", "") .. "Gym"] = selected
	end)

