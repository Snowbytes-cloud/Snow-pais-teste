        agilityLabel.Text = "Agilidad: " .. AbbrevNumber(cAgi) .. " | +" .. AbbrevNumber(dAgi)

        if tick() - lastUpdate >= 6 then
            lastUpdate = tick()
            local sSec, dSec, rSec = dStr / elapsedTime, dDur / elapsedTime, dReb / elapsedTime
            local h, d, m = 3600, 86400, 2592000
            projectedStrengthLabel.Text = "Ritmo Fuerza: " .. AbbrevNumber(math.floor(sSec*h)) .. "/h | " .. AbbrevNumber(math.floor(sSec*d)) .. "/d"
            projectedDurabilityLabel.Text = "Ritmo Durabilidad: " .. AbbrevNumber(math.floor(dSec*h)) .. "/h | " .. AbbrevNumber(math.floor(dSec*d)) .. "/d"
            projectedRebirthsLabel.Text = "Ritmo Renacimientos: " .. AbbrevNumber(math.floor(rSec*h)) .. "/h | " .. AbbrevNumber(math.floor(rSec*d)) .. "/d | " .. AbbrevNumber(math.floor(rSec*m)) .. "/mes"
        end
    end
end)

